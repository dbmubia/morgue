	<subDataset name="getservices" uuid="32af67c0-899a-4aa6-919c-11111a28fa46">
		<queryString>
			<![CDATA[SELECT
     invoice_tb.`finance_serial` AS Record_No,
     invoice_tb.`service_code_finance` AS Code,
     services.`service_description` AS Description,
     invoice_tb.`date` AS Date,
     invoice_tb.`cost_of_service` AS Cost

FROM
     `deceased_tb` deceased_tb INNER JOIN `invoice_tb` invoice_tb ON deceased_tb.`AdminNo` = invoice_tb.`admin_no_finance`
     INNER JOIN `services` services ON invoice_tb.`service_code_finance` = services.`service_code`
WHERE
     deceased_tb.AdminNo = '29']]>
		</queryString>
		<field name="Record_No" class="java.lang.Integer"/>
		<field name="Code" class="java.lang.String"/>
		<field name="Description" class="java.lang.String"/>
		<field name="Date" class="java.sql.Date">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="Cost" class="java.lang.Double"/>
		<variable name="total" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{Cost}]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="admin" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     deceased_tb.`AdminNo` AS deceased_tb_AdminNo,
     deceased_tb.`title` AS deceased_tb_title,
     deceased_tb.`first_name` AS deceased_tb_first_name,
     deceased_tb.`middle_name` AS deceased_tb_middle_name,
     deceased_tb.`last_name` AS deceased_tb_last_name,
     deceased_tb.`deceased_id` AS deceased_tb_deceased_id
FROM
     `deceased_tb` deceased_tb
WHERE
     deceased_tb.AdminNo =$P{admin}]]>
	</queryString>
